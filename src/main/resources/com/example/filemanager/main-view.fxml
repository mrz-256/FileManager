<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox id="root_vbox" maxWidth="1200.0" prefHeight="675.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.filemanager.UIController">
    <children>
        <ToolBar id="button_bar" prefHeight="40.0" prefWidth="1200.0">
            <items>
                <Button id="back-button" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../icons/icon_3.png" />
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="back" />
                    </tooltip>
                </Button>
                <Button id="forward-button" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../icons/icon_2.png" />
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="forward" />
                    </tooltip>
                </Button>
                <Button id="show-as-boxes" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../icons/icon_0.png" />
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="display files as boxes" />
                    </tooltip>
                </Button>
                <Button id="show-as-list" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../icons/icon_1.png" />
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="display boxes as list" />
                    </tooltip>
                </Button>
                <Button id="split-tab-button" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../icons/icon_5.png" />
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="split/unsplit tab" />
                    </tooltip>
                </Button>
                <Button id="search-button" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../icons/icon_6.png" />
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="search for files" />
                    </tooltip>
                </Button>
                <Button id="filter-button" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../icons/icon_8.png" />
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="filter shown files" />
                    </tooltip>
                </Button>
                <SplitMenuButton alignment="CENTER_RIGHT" mnemonicParsing="false" popupSide="LEFT" prefHeight="11.0" prefWidth="93.0" text="Settings" textAlignment="RIGHT">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Action 1" />
                        <MenuItem mnemonicParsing="false" text="Action 2" />
                    </items>
                </SplitMenuButton>
            </items>
        </ToolBar>
        <HBox id="main_hbox" prefHeight="625.0" prefWidth="1200.0">
            <ScrollPane id="global_files_scrollpane" prefHeight="625.0" prefWidth="200.0">
                <VBox prefHeight="Infinity" prefWidth="200.0" HBox.hgrow="ALWAYS">
                    <Label text="Places" />
                    <VBox id="places_vbox" fx:id="places" prefHeight="200.0" prefWidth="200.0" />
                    <Label text="Recent" />
                    <VBox id="global_files_scrollpane" fx:id="recent" prefHeight="200.0" prefWidth="100.0" />
                </VBox>
            </ScrollPane>
            <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="ALL_TABS" HBox.hgrow="ALWAYS">
            <tabs>
               <Tab text="home">
                 <content>
                   <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        <children>
                           <SplitPane dividerPositions="0.29797979797979796" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                             <items>
                               <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="100.0" minWidth="100.0" prefHeight="160.0" prefWidth="100.0" />
                               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" />
                             </items>
                           </SplitPane>
                        </children></AnchorPane>
                 </content>
               </Tab>
            </tabs></TabPane>
        </HBox>
    </children>
</VBox>
